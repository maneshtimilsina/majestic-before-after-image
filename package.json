{
	"name": "majestic-before-after-image",
	"version": "1.0.11",
	"description": "Majestic Before After Image",
	"main_file": "majestic-before-after-image.php",
	"author": "Manesh Timilsina",
	"license": "GPL-2.0-or-later",
	"keywords": [
		"wordpress",
		"wordpress-plugin"
	],
	"homepage": "https://github.com/maneshtimilsina/majestic-before-after-image",
	"repository": {
		"type": "git",
		"url": "https://github.com/maneshtimilsina/majestic-before-after-image.git"
	},
	"bugs": {
		"url": "https://github.com/maneshtimilsina/majestic-before-after-image/issues"
	},
	"browserslist": "> 1%, not dead",
	"packtor": {
		"files": [
			"**/*",
			"!*.js",
			"!*.json",
			"!*.lock",
			"!*.yaml",
			"!resources/**/*"
		]
	},
	"wpDeployer": {
		"repoType": "plugin",
		"username": "maneshtimilsina",
		"buildDir": "deploy/majestic-before-after-image",
		"deployAssets": false
	},
	"npmpackagejsonlint": {
		"extends": "@wordpress/npm-package-json-lint-config"
	},
	"devDependencies": {
		"@babel/core": "^7.24.7",
		"@babel/preset-env": "^7.24.7",
		"@wordpress/eslint-plugin": "^14.6.0",
		"@wordpress/npm-package-json-lint-config": "^5.1.0",
		"@wordpress/prettier-config": "^2.16.0",
		"browser-sync": "^3.0.2",
		"dotenv": "^16.4.5",
		"easy-replace-in-files": "^1.0.3",
		"eslint": "^8.41.0",
		"gulp": "^4.0.2",
		"gulp-autoprefixer": "^8.0.0",
		"gulp-babel": "^8.0.0",
		"gulp-environments": "^1.0.1",
		"gulp-plumber": "^1.2.1",
		"gulp-sass": "^5.1.0",
		"gulp-sourcemaps": "^3.0.0",
		"node-wp-i18n": "^1.2.7",
		"npm-package-json-lint": "^8.0.0",
		"packtor": "^1.0.2",
		"prettier": "npm:wp-prettier@^3.0.3",
		"sass": "^1.77.6",
		"shx": "^0.3.4",
		"wp-deployer": "^1.0.3"
	},
	"scripts": {
		"dev": "pnpm run build && gulp",
		"build": "gulp build",
		"prod": "gulp build --env production",
		"eslint": "eslint --quiet .",
		"eslint:fix": "eslint --quiet --fix .",
		"format": "prettier --write \"resources/**/*.scss\" *.json",
		"pot": "wpi18n makepot --domain-path=languages --exclude=vendor,deploy,node_modules",
		"version": "easy-replace-in-files",
		"prewpdeploy": "pnpm run deploy",
		"wpdeploy": "wp-deployer",
		"lint-pkg-json": "npmPkgJsonLint .",
		"ready:build": "shx rm -rf assets/ && pnpm run prod",
		"ready:vendor": "shx rm -rf vendor/ && composer install --no-dev --no-scripts -o",
		"predeploy": "pnpm run ready:vendor && pnpm run ready:build",
		"deploy": "packtor"
	}
}
